{
  "name": "testmonitor/topdesk-client",
  "description": "The TestMonitor TOPdesk client.",
  "keywords": ["testmonitor", "topdesk", "client"],
  "authors": [
    {
      "name": "Thijs Kok",
      "email": "tkok@testmonitor.com",
      "homepage": "https://www.testmonitor.com/",
      "role": "Lead Developer"
    },
    {
      "name": "Stephan Grootveld",
      "email": "sgrootveld@testmonitor.com",
      "homepage": "https://www.testmonitor.com/",
      "role": "Developer"
    },
    {
      "name": "Frank Keulen",
      "email": "fkeulen@testmonitor.com",
      "homepage": "https://www.testmonitor.com/",
      "role": "Developer"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.1",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "^10.0",
    "squizlabs/php_codesniffer": "^3.7",
    "scrutinizer/ocular": "^1.9",
    "symfony/console": "^6.0",
    "symfony/finder": "^6.0",
    "symfony/event-dispatcher": "^6.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "TestMonitor\\TOPdesk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TestMonitor\\TOPdesk\\Tests\\": "tests/"
    }
  },
  "prefer-stable": true,
  "license": "MIT"
}
